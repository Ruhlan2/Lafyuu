<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.ui.account.profile.ProfileFragment">


    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginStart="16dp"
        android:fontFamily="sans-serif-black"
        android:text="Account"
        android:textColor="@color/blue_d"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.google.android.material.divider.MaterialDivider
        android:id="@+id/materialDivider"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />


    <com.google.android.material.button.MaterialButton
        android:id="@+id/profilebtn"
        style="@style/Widget.Material3.Button.OutlinedButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_semibold"
        android:paddingVertical="15dp"
        android:text="Profile"
        android:textAlignment="viewStart"
        android:textColor="@color/blue_d"
        android:textSize="20sp"
        app:cornerRadius="0dp"
        app:icon="@drawable/profile"
        app:iconPadding="16dp"
        app:iconSize="25dp"
        app:rippleColor="@color/white"
        app:iconTint="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/materialDivider"
        app:strokeWidth="0dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton6"
        style="@style/Widget.Material3.Button.OutlinedButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="15dp"
        android:fontFamily="@font/inter_semibold"
        android:text="Order"
        android:textAlignment="viewStart"
        android:textColor="@color/blue_d"
        app:rippleColor="@color/white"
        android:textSize="20sp"
        app:cornerRadius="0dp"
        app:icon="@drawable/cart_check_svgrepo_com"
        app:iconPadding="16dp"
        app:iconSize="25dp"
        app:iconTint="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profilebtn"
        app:strokeWidth="0dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/addressBtn"
        style="@style/Widget.Material3.Button.OutlinedButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="15dp"
        android:fontFamily="@font/inter_semibold"
        android:text="Address"
        app:rippleColor="@color/white"
        android:textAlignment="viewStart"
        android:textColor="@color/blue_d"
        android:textSize="20sp"
        app:cornerRadius="0dp"
        app:icon="@drawable/address_location_map_svgrepo_com"
        app:iconPadding="16dp"
        app:iconSize="27dp"
        app:iconTint="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/materialButton6"
        app:strokeWidth="0dp" />

    <com.google.android.material.button.MaterialButton
        style="@style/Widget.Material3.Button.OutlinedButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter_semibold"
        android:text="Payment"
        android:paddingVertical="15dp"
        android:textAlignment="viewStart"
        android:textColor="@color/blue_d"
        android:textSize="20sp"
        app:rippleColor="@color/white"
        app:cornerRadius="0dp"
        app:icon="@drawable/card_transfer_svgrepo_com"
        app:iconPadding="16dp"
        app:iconSize="27dp"
        app:iconTint="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addressBtn"
        app:strokeWidth="0dp" />

</androidx.constraintlayout.widget.ConstraintLayout>